  "scripts": {
    "dev": "vite",
    "start": "yarn dev",
    "start:test": "vite --mode test",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "lint": "eslint .",
    "pre-commit": "yarn prettier:check && yarn lint && yarn type-check",
    "prettier:check": "prettier --check {src,cypress}/**/*.{js,ts,tsx,json}",
    "prettier:write": "prettier --write {src,cypress}/**/*.{js,ts,tsx,json}",
    "type-check": "tsc --noEmit && tsc --noEmit -p cypress/tsconfig.json",
    "check": "yarn prettier:check && yarn lint && yarn type-check",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "cypress:open": "env-cmd -f ./.env.development cypress open --browser chrome",
    "cypress": "concurrently -k -s first \"yarn start:test\" \"yarn cypress:run\"",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome",
    "postinstall": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
